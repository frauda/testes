<p id="notice"><%= notice %></p>

<h1>Listing Consulta</h1>

<table>
  <thead>
    <tr>
      <th>Data hora</th>
      <th>Efetuado</th>
      <th>Valor</th>
      <th>Pagamento</th>
      <th>Pessoa</th>
      <th>Convenio</th>
      <th>Atendente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consulta.each do |consultum| %>
      <tr>
        <td><%= consultum.data_hora %></td>
        <td><%= consultum.efetuado %></td>
        <td><%= consultum.valor %></td>
        <td><%= consultum.pagamento %></td>
        <td><%= consultum.pessoa %></td>
        <td><%= consultum.convenio %></td>
        <td><%= consultum.atendente %></td>
        <td><%= link_to 'Show', consultum %></td>
        <td><%= link_to 'Edit', edit_consultum_path(consultum) %></td>
        <td><%= link_to 'Destroy', consultum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consultum', new_consultum_path %>
